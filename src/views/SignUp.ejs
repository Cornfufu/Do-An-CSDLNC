<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CSDL</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/index.css">
</head>

<body>
    <div class="topnav">
        <a class="active" href="/home">Home</a>
        <a href="/Login">Login</a>
        <a href="/SignUp">SignUp</a>
        <form class="search-wrapper" action="/search">
            <input id="search" type="text" class="input" name="keyword" placeholder="search..." data-search />
            <button id="searchB" type="submit" class="fa fa-search" data-search>search</button>
        </form>
    </div>

    <form action="/SignUp/SignUpSucess">
        <div class="form-group">
            <label for="exampleInputEmail1">Tên tài khoản</label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="TenTK"
                placeholder="Nhập tên tài khoản">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Mật khẩu</label>
            <input type="password" class="form-control" id="exampleInputPassword1" name="MatKhau"
                placeholder="Nhập mật khẩu">
        </div>
        <div class="form-group">
            <label for="exampleFormControlSelect1">Loại tài khoản</label>
            <select class="form-control" id="exampleFormControlSelect1" name="LoaiTK">
                <option value="3">1.Khách hàng</option>
                <option value="4">2.Tài xế</option>
                <option value="5">3.Đối tác</option>
            </select>
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">CMND</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="CMND" placeholder="Nhập CMND">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Số điện thoại</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="Sdt" placeholder="Nhập Số điện thoại">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Email</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="Email" placeholder="Nhập Email">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Số nhà</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="SoNha" placeholder="Nhập Số nhà">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Đường</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="Duong" placeholder="Nhập Đường">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Quận</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="Quan" placeholder="Nhập Quận">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Thành phố</label>
            <input type="text" class="form-control" id="exampleInputEmail1" name="ThanhPho"
                placeholder="Nhập Thành phố">
        </div>
        <button type="submit" class="btn btn-primary">Gửi</button>
    </form>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
        integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
        integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
</body>

</html>